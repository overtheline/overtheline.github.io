{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",

        "strict": true, 
        "noImplicitAny": true,
        "strictNullChecks": true,
        "noImplicitThis": true,

        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,

        "sourceMap": true
    },
    "exclude": [
        "node_modules",
        "draft-2017"
    ]
}